<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Despesas</title>
    
    <script>
        // Função para redirecionar para a nova página
        function redirecionar() {
            window.location.href = "https://form.respondi.app/CdaNqGhQ";
        }

        // Data e hora para o redirecionamento (no exemplo, 31/07/2023 às 12:00)
        var dataRedirecionamento = new Date("2023-07-31T12:00:00").getTime();

        // Função para verificar a data e redirecionar, se necessário
        function verificarDataRedirecionamento() {
            var dataAtual = new Date().getTime();

            if (dataAtual >= dataRedirecionamento) {
                redirecionar();
            }
        }

        // Verificar a data ao carregar a página
        verificarDataRedirecionamento();
    </script>
</head>
<body>
    <!-- Conteúdo da página de manutenção aqui -->
</body>
</html>
